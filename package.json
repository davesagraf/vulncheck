    "hygen": "^6.2.11",
    "import-sort-style-module": "6.0.0",
    "jest": "28.1.0",
    "jira.js": "^1.6.2",
    "jsonwebtoken": "8.5.1",
    "libreoffice-convert": "^1.4.1",
    "logform": "2.4.2",
    "minimatch": "5.0.1",
    "morgan": "1.10.0",
    "multer": "2.0.0-rc.3",
    "nest-router": "1.0.9",
    "nest-winston": "^1.6.2",
    "nestjs-i18n": "^8.2.2",
    "nestjs-s3": "^1.0.1",
    "nyc": "15.1.0",
    "node-notifier": "10.0.1",
    "node-request-context": "1.0.5",
