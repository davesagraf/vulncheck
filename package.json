{
  "name": "vulncheck",
  "version": "0.0.1",
  "description": "check repo vulnerabilities",
  "author": "dave",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davesagraf/vulncheck.git"
  },
  "keywords": [
    "security",
    "CVE",
    "vulnerability"
  ],
  "bugs": {
    "url": "https://github.com/davesagraf/vulncheck/issues"
  },
  "homepage": "https://github.com/davesagraf/vulncheck#readme",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.10.4",
    "@types/bull": "3.15.8",
    "@types/cache-manager": "4.0.2",
    "@types/cron": "2.0.0",
    "@types/express": "4.17.13",
    "@types/ioredis": "4.28.10",
    "@types/jest": "27.5.1",
    "@types/supertest": "2.0.12",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "import-sort-style-module": "6.0.0",
    "prettier": "3.1.1",
    "prettier-plugin-import-sort": "0.0.7",
    "pretty-error": "4.0.0",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.41",
    "nodemon": "^2.0.21",
    "swc-loader": "^0.2.3",
    "nyc": "15.1.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1"
  },
  "dependencies": {
    "nodemailer-ntlm-auth": "^1.0.4",
    "@babel/plugin-proposal-private-methods": "7.18.6",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "0.5.5",
    "@nestjs/cli": "9.2.0",
    "@nestjs/common": "8.4.5",
    "@nestjs/core": "8.4.5",
    "@nestjs/platform-express": "8.4.5",
    "@nestjs/platform-socket.io": "8.4.5",
    "@nestjs/schedule": "1.1.0",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/testing": "8.4.5",
    "@nestjs/websockets": "9.0.11",
    "@compodoc/compodoc": "1.1.21",
    "activedirectory2": "2.1.0",
    "ad-promise": "1.6.8",
    "ansi-regex": "6.0.1",
    "aws-sdk": "^2.1270.0",
    "bcryptjs": "2.4.3",
    "bull": "4.8.3",
    "bull-board": "2.1.3",
    "cache-manager": "4.1.0",
    "cache-manager-ioredis": "2.1.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.13.2",
    "colors": "1.4.0",
    "cross-env": "7.0.3",
    "dotenv": "16.0.1",
    "easy-template-x": "0.12.0",
    "ejs": "^3.1.8",
