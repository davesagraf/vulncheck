    "swagger-ui-express": "4.4.0",
    "tapable": "2.2.1",
    "ts-jest": "28.0.2",
    "ts-loader": "9.3.0",
    "ts-node": "10.7.0",
    "tsconfig-paths": "4.0.0",
    "typeorm": "0.2.38",
    "typescript": "4.3.5",
    "mime": "3.0.0",
    "winston": "^3.10.0",
    "winston-syslog": "^2.7.0",
    "date-fns": "2.30.0",
    "webpack-node-externals": "3.0.0",
    "copy-webpack-plugin": "11.0.0",
    "xml2js-parser": "1.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
